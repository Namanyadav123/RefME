<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/l7"

    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/books3eee1"
        app:cub_border_radius="4dp"
        app:cub_color="#ad6eae"
        android:gravity="clip_horizontal"
        android:text="Electromech Energy Conversion And Transformers"
        app:cub_hover_color="#6918c6"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:cub_border_color="#6d0d7c"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,RtlHardcoded"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/books3eee3"
        app:cub_border_radius="4dp"
        app:cub_color="#ad6eae"
        android:gravity="clip_horizontal"
        android:text="Electronic Devices And Circuits"
        app:cub_hover_color="#c61818"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:cub_border_color="#6d0d7c"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,RtlHardcoded"
        android:layout_marginRight="17dp"
        android:layout_marginEnd="17dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/books3eee2"
        app:cub_border_radius="4dp"
        app:cub_color="#ad6eae"
        android:gravity="clip_horizontal"
        android:text="Engineering Analysis And Design"
        app:cub_hover_color="#c61818"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:cub_border_color="#6d0d7c"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,RtlHardcoded"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/books3eee5"
        app:cub_border_radius="4dp"
        app:cub_color="#ad6eae"
        android:gravity="clip_horizontal"
        android:text="Numerical Engineering Optimization Methods"
        app:cub_hover_color="#6918c6"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:cub_border_color="#6d0d7c"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,RtlHardcoded"
        android:layout_marginLeft="35dp"
        android:layout_marginStart="35dp"
        android:layout_alignBaseline="@+id/books3eee4"
        android:layout_alignBottom="@+id/books3eee4"
        android:layout_toRightOf="@+id/books3eee4"
        android:layout_toEndOf="@+id/books3eee4" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
    android:id="@+id/books3eee6"
    app:cub_border_radius="4dp"
    app:cub_color="#ad6eae"
    android:gravity="clip_horizontal"
        android:text="Engineering Economics           Part-I(Seema Singh)"
    app:cub_hover_color="#6918c6"
        android:layout_width="110dp"
        android:layout_height="110dp"
    app:cub_border_color="#6d0d7c"
    tools:ignore="HardcodedText,ObsoleteLayoutParam,RtlHardcoded"
    android:layout_marginTop="26dp"
    android:layout_below="@+id/books3eee4"
    android:layout_toLeftOf="@+id/books3eee2"
    android:layout_toStartOf="@+id/books3eee2"
    android:layout_marginRight="11dp"
    android:layout_marginEnd="11dp" />
    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/books3eee7"
        app:cub_border_radius="4dp"
        app:cub_color="#ad6eae"
        android:gravity="clip_horizontal"
        android:text="Engineering Economics           Part-II(Seema Singh)"
        app:cub_hover_color="#6918c6"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:cub_border_color="#6d0d7c"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,RtlHardcoded"
        android:layout_alignBaseline="@+id/books3eee6"
        android:layout_alignBottom="@+id/books3eee6"
        android:layout_alignLeft="@+id/books3eee3"
        android:layout_alignStart="@+id/books3eee3" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/books3eee4"
        app:cub_border_radius="4dp"
        app:cub_color="#ad6eae"
        android:gravity="clip_horizontal"
        android:text="Circuits And Systems"
        app:cub_hover_color="#6918c6"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:cub_border_color="#6d0d7c"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,RtlHardcoded"
        android:layout_marginLeft="34dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/books3eee1"
        android:layout_alignLeft="@+id/books3eee6"
        android:layout_alignStart="@+id/books3eee6" />
</RelativeLayout>
