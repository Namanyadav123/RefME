<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="io.app.hasura.imad.namanyadav123.mydtu.Notes1">


    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent"
        tools:ignore="ScrollViewSize">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:background="@drawable/phy"
                android:layout_height="wrap_content" />

            <com.cuboid.cuboidcirclebutton.CuboidButton
                android:id="@+id/p1"
                app:cub_border_radius="4dp"
                app:cub_border_color="#06e448"
                app:cub_color="#aaed0b"
                android:text="Visit"
                app:cub_hover_color="#10eadc"
                android:layout_width="80dp"
                android:layout_height="100dp"
                tools:ignore="HardcodedText"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />


        </RelativeLayout>



        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:background="@drawable/math"
                android:layout_height="wrap_content" />

            <com.cuboid.cuboidcirclebutton.CuboidButton
                android:id="@+id/p2"
                app:cub_border_radius="4dp"
                app:cub_border_color="#06e448"
                app:cub_color="#aaed0b"
                android:text="Visit"
                app:cub_hover_color="#10eadc"
                android:layout_width="80dp"
                android:layout_height="100dp"
                tools:ignore="HardcodedText"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:background="@drawable/chem"
                android:layout_height="wrap_content" />

            <com.cuboid.cuboidcirclebutton.CuboidButton
                android:id="@+id/p3"
                app:cub_border_radius="4dp"
                app:cub_border_color="#06e448"
                app:cub_color="#aaed0b"
                android:text="Visit"
                app:cub_hover_color="#10eadc"
                android:layout_width="80dp"
                android:layout_height="100dp"
                tools:ignore="HardcodedText"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:background="@drawable/bme"
                android:layout_height="wrap_content" />

            <com.cuboid.cuboidcirclebutton.CuboidButton
                android:id="@+id/p4"
                app:cub_border_radius="4dp"
                app:cub_border_color="#06e448"
                app:cub_color="#aaed0b"
                android:text="Visit"
                app:cub_hover_color="#10eadc"
                android:layout_width="80dp"
                android:layout_height="100dp"
                tools:ignore="HardcodedText"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:background="@drawable/omm"
                android:layout_height="wrap_content" />

            <com.cuboid.cuboidcirclebutton.CuboidButton
                android:id="@+id/p5"
                app:cub_border_radius="4dp"
                app:cub_border_color="#06e448"
                app:cub_color="#aaed0b"
                android:text="Visit"
                app:cub_hover_color="#10eadc"
                android:layout_width="80dp"
                android:layout_height="100dp"
                tools:ignore="HardcodedText"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />


        </RelativeLayout>






    </LinearLayout>


</ScrollView>
