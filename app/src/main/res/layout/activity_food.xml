<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    android:theme="@style/Theme.AppCompat.NoActionBar"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@drawable/a13"
    android:layout_alignParentBottom="true"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/circle1"
        app:cub_border_radius="4dp"
        app:cub_color="#acb696"
        android:gravity="clip_horizontal"
        android:text="MONDAY"
        app:cub_hover_color="#0b0606"
        app:cub_border_color="#6b6f78"
        android:textColor="#000000"

        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_marginTop="141dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="132dp"
        android:layout_marginEnd="132dp"
        android:layout_width="60dp"
        android:layout_height="60dp" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/cuboidButton"
        app:cub_border_radius="4dp"
        app:cub_color="#acb696"
        android:gravity="clip_horizontal"
        android:text="TUESDAY"
        app:cub_hover_color="#0b0606"
        android:layout_width="65dp"
        app:cub_border_color="#6b6f78"
        android:textColor="#000000"

        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_height="65dp"
        android:layout_marginTop="65dp"
        android:layout_marginRight="52dp"
        android:layout_marginEnd="52dp"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/circle1"
        android:layout_alignEnd="@+id/circle1" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/cuboidButton4"
        app:cub_border_radius="4dp"
        app:cub_color="#acb696"
        android:gravity="clip_horizontal"
        android:text="THURSDAY"
        app:cub_hover_color="#0b0606"
        app:cub_border_color="#6b6f78"
        android:textColor="#000000"

        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_alignBottom="@+id/cuboidButton"
        android:layout_toRightOf="@+id/cuboidButton"
        android:layout_toEndOf="@+id/cuboidButton"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginBottom="16dp"
        android:layout_height="70dp"
        android:layout_width="75dp" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/cuboidButton5"
        app:cub_border_radius="4dp"
        app:cub_color="#acb696"
        android:gravity="clip_horizontal"
        android:text="FRIDAY"
        app:cub_hover_color="#0b0606"
        app:cub_border_color="#6b6f78"
        android:textColor="#000000"

        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/cuboidButton4"
        android:layout_toStartOf="@+id/cuboidButton4"
        android:layout_width="60dp"
        android:layout_height="60dp" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/cuboidButton6"
        app:cub_border_radius="4dp"
        app:cub_color="#acb696"
        android:gravity="clip_horizontal"
        android:text="SATURDAY"
        app:cub_hover_color="#0b0606"
        app:cub_border_color="#6b6f78"
        android:textColor="#000000"

        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/cuboidButton3"
        android:layout_alignStart="@+id/cuboidButton3"
        android:layout_height="70dp"
        android:layout_width="75dp" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/cuboidButton3"
        app:cub_border_radius="4dp"
        app:cub_color="#acb696"
        android:gravity="clip_horizontal"
        android:text="WEDNESDAY"
        app:cub_hover_color="#0b0606"
        app:cub_border_color="#6b6f78"
        android:textColor="#000000"

        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignTop="@+id/circle1"
        android:layout_toLeftOf="@+id/cuboidButton5"
        android:layout_toStartOf="@+id/cuboidButton5"
        android:layout_marginRight="46dp"
        android:layout_marginEnd="46dp"
        android:layout_marginTop="13dp" />

    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:id="@+id/cuboidButton7"
        app:cub_border_radius="4dp"
        app:cub_color="#acb696"
        android:gravity="clip_horizontal"
        android:text="SUNDAY"
        app:cub_hover_color="#0b0606"
        android:layout_width="65dp"
        app:cub_border_color="#6b6f78"
        android:textColor="#000000"

        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_height="65dp"
        android:layout_above="@+id/cuboidButton3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>